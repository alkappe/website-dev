---
# Questo file genera automaticamente il json che viene letto nello script dell'home page per generare gli artwork
# DO NOT EDIT THIS FILE
---
[
  {% for project in site.categories.project %}
   {
     "title"        : "{{ project.title }}",
     "author"       : "{{ project.author }}",
     "description"  : "{{ project.description }}",
     "url"         : "{{ project.url }}",
     "href"         : "{{ site.url }}/js/projects/{{ project.href }}",
     {% if project.includes != null %}
     "includes"     : [
       {% for inc in project.includes %}
       "{{ inc }}"
       {% unless forloop.last %},{% endunless %}
       {% endfor %}
     ]
     {% endif %}
   }
   {% unless forloop.last %},{% endunless %}
   {% endfor %}
]
